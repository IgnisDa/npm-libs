{
  "dependencies": {
    "required": [
      "@remix-run/react",
      "clsx",
      "core-js",
      "react",
      "react-dom",
      "react-icons",
      "regenerator-runtime",
      "ufo"
    ],
    "development": [
      "@nrwl/eslint-plugin-nx",
      "@nrwl/jest",
      "@nrwl/linter",
      "@nrwl/web",
      "@testing-library/react",
      "@types/jest",
      "@types/lodash",
      "@types/react",
      "@types/react-dom",
      "@typescript-eslint/eslint-plugin",
      "@typescript-eslint/parser",
      "babel-jest",
      "eslint-config-prettier",
      "eslint-plugin-import",
      "eslint-plugin-jsx-a11y",
      "eslint-plugin-react",
      "eslint-plugin-react-hooks",
      "jest",
      "react-test-renderer",
      "sass",
      "ts-jest",
      "ts-node"
    ]
  },
  "projectType": "library",
  "sourceRoot": "packages/remix-pagination/src",
  "tags": [],
  "targets": {
    "build": {
      "dependsOn": [
        {
          "projects": "self",
          "target": "build:scss"
        }
      ],
      "executor": "@nrwl/web:rollup",
      "options": {
        "assets": [
          {
            "glob": "packages/remix-pagination/README.md",
            "input": ".",
            "output": "."
          },
          {
            "glob": "packages/remix-pagination/src",
            "input": ".",
            "output": "."
          },
          {
            "glob": "LICENSE",
            "input": ".",
            "output": "."
          }
        ],
        "compiler": "babel",
        "entryFile": "packages/remix-pagination/src/index.ts",
        "external": ["react/jsx-runtime"],
        "format": ["cjs"],
        "outputPath": "dist/packages/remix-pagination",
        "project": "packages/remix-pagination/package.json",
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "tsConfig": "packages/remix-pagination/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "build:scss": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "sass ./src",
        "cwd": "packages/remix-pagination"
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["packages/remix-pagination/**/*.{ts,tsx,js,jsx}"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "packages/remix-pagination/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["coverage/packages/remix-pagination"]
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "allowEmptyRelease": true
      }
    }
  }
}
