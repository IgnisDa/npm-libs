{
  "dependencies": {
    "required": ["@swc/helpers", "chalk", "stream-chain", "stream-json"],
    "development": [
      "@nrwl/devkit",
      "@nrwl/js",
      "@swc-node/register",
      "@swc/cli",
      "@swc/core",
      "@swc/helpers",
      "@swc/jest",
      "@types/glob",
      "@types/json-stable-stringify",
      "@types/stream-chain",
      "@types/stream-json",
      "glob",
      "json-stable-stringify",
      "jsonc-eslint-parser",
      "tslib",
      "typescript-json-schema"
    ]
  },
  "projectType": "library",
  "sourceRoot": "packages/nx-rust/src",
  "tags": ["publish-npm"],
  "targets": {
    "build": {
      "dependsOn": [
        {
          "projects": "self",
          "target": "generate-schema"
        }
      ],
      "executor": "@nrwl/js:swc",
      "options": {
        "assets": [
          "packages/nx-rust/*.md",
          {
            "glob": "**/!(*.ts)",
            "input": "./packages/nx-rust/src",
            "output": "./src"
          },
          {
            "glob": "**/*.d.ts",
            "input": "./packages/nx-rust/src",
            "output": "./src"
          },
          {
            "glob": "generators.json",
            "input": "./packages/nx-rust",
            "output": "."
          },
          {
            "glob": "executors.json",
            "input": "./packages/nx-rust",
            "output": "."
          }
        ],
        "main": "packages/nx-rust/src/index.ts",
        "outputPath": "dist/packages/nx-rust",
        "tsConfig": "packages/nx-rust/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "generate-schema": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ts-node src/generate-schema.ts",
        "cwd": "packages/nx-rust"
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/nx-rust/**/*.ts",
          "packages/nx-rust/generators.json",
          "packages/nx-rust/executors.json",
          "packages/nx-rust/package.json"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "packages/nx-rust/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["coverage/packages/nx-rust"]
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional"
      }
    }
  }
}
